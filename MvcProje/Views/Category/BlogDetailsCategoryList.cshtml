@model IEnumerable<EntityLayer.Concrete.Category>
<div class="sidebar-box bg-dark">
    <h3 class="heading text-white">Kategoriler</h3>
    <ul class="categories">
        @foreach (var x in Model)
        {
            <li><a style="color:lightgrey" href="/Blog/BlogByCategory/@x.CategoryID">@x.CategoryName</a></li>
        }
    </ul>
</div>